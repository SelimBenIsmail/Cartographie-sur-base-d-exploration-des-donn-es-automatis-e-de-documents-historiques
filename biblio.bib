@article{demolon_les_1979,
	title = {Les origines médiévales de la ville de Douai ; rapport provisoire des fouilles de la "fonderie de canons"},
	volume = {61},
	rights = {free},
	url = {https://www.persee.fr/doc/rnord_0035-2624_1979_num_61_241_3584},
	doi = {10.3406/rnord.1979.3584},
	abstract = {En 1976, des projets de constructions au centre du Castrum Duacum, là où les textes situent le premier noyau urbain de Douai (centre politique, religieux), ont incité à une reconnaissance archéologique, puis à une fouille complète. Sept périodes principales ont été distinguées. Période I : Époque mérovingienne ; deux cabanes excavées ont été découvertes. Période {II} : Époque carolingienne où l'on relève essentiellement des trous de poteaux et des fosses. Période {III} : Agglomération {IXème}-Xème siècles, où l'activité agricole prédomine. Les structures sont très nombreuses et souvent enchevêtrées. Les éléments organiques (bois, végétaux, insectes, grains, cuirs...) sont dans un état de conservation exceptionnel. Période {IV} : Ensemble de structures (bâtiment en bois partiellement emmotté, fossé puits...) correspondant au Castrum du Comte de Flandres (Xème-{XIème} siècles). Période V : Donjon en pierres sur motte, bâtiment annexe et rempart urbain (état du {XIIème} siècle). Période {VI} : Château du Bas Moyen Age et de la période moderne. Période {VII} : Fonderie de canons créée par Louis {XIV} en 1669, et qui subsistera jusque vers 1870. Une série stratigraphique continue du {VIème} siècle jusqu'à l'époque contemporaine, des éléments organiques exceptionnellement conservés, donnent à cette fouille un intérêt particulier pour l'étude de l'origine et du développement du phénomène urbain en Flandres.},
	pages = {301--329},
	number = {241},
	journaltitle = {Revue du Nord},
	author = {Demolon, Pierre and Barbieux, José},
	urldate = {2022-07-25},
	date = {1979},
	note = {Publisher: Persée - Portail des revues scientifiques en {SHS}},
	file = {Snapshot:/Users/selimbenismail/Zotero/storage/GM7MVZZX/rnord_0035-2624_1979_num_61_241_3584.html:text/html},
}

