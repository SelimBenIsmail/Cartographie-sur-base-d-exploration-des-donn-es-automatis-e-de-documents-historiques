@article{leguay_propriete_1989,
	title = {La propriété et le marché de l'immobilier à la fin du Moyen Âge dans le royaume de France et dans les grands fiefs périphériques},
	volume = {122},
	rights = {free},
	url = {https://www.persee.fr/doc/efr_0000-0000_1989_act_122_1_4594},
	abstract = {Jean-Pierre Leguay, La propriété et le marché de l'immobilier à la fin du Moyen Âge dans le royaume de France et dans les grands fiefs périphériques, p. 135-199. L'examen de ce sujet vaste et complexe repose sur une documentation fragmentaire provenant des cartulaires des établissements religieux, des registres fiscaux et des archives privées (testaments, comptabilités familiales etc.). L'immobilier, appelé « l'hiretage » dans certains documents, recouvre toutes les formes de la propriété bâtie, du vaste hôtel particulier aux logements de rapport et aux locaux à usage artisanal et commercial, les espaces non édifiés à promouvoir, les jardins et les vergers, les champs intra et extra-muros, et même les rentes constituées perpétuelles et hypothéquées sur des biens-fonds. Au cours des {XIIIe}-{XVe} siècles, dans des proportions variables suivant la conjoncture et les cadres urbains, beaucoup de bourgeois sont intéressés par ce type de possession et de spéculation: les « patriciens » d'abord, gros marchands, financiers, hommes de loi, mais aussi de simples artisans et boutiquiers disposant, momentanément, de sommes à placer. L'étude intéresse tous les aspects de ce négoce : les motivations des investisseurs, les biens recherchés, leur localisation, la concentration des fortunes par périodes et par secteurs urbains, l'évaluation des patrimoines.},
	pages = {135--199},
	number = {1},
